{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Class",
	"patterns": [
		{
			"include": "#Comment"
		},
		{
			"include": "#KeywordClassName"
		},
		{
			"include": "#Keyword"
		},
		{
			"include": "#IntValue"
		},
		{
			"include": "#StringValue"
		}
	],
	"repository": {
		"Keyword": {
			"patterns": [{
				"name": "keyword.class",
				"match": "#KeywordRegexString#"
			}]
		},
		"KeywordClassName": {
			"patterns": [{
				"begin": "(?<=^|[^A-Za-z0-9_])#WordClassKeyword#[ ]*(?=$|[^A-Za-z0-9_])",
				"beginCaptures": {
					"1": {
						"name": "keyword.class"
					}
				},
				"end": "([ ]*#Name#)|.",
				"endCaptures": {
					"4" : {
						"name": "entity.name.type.class"
					}
				}
			}]
		},
		"ClassName": {
			"patterns": [{
				"match": "#ClassNameRegexString#",
				"captures": {
					"3" : { "name": "keyword.class" },
					"6" : { "name": "entity.name.type.class" },
					"10" : { "name": "entity.name.type.class" },
					"12" : { "name": "keyword.class" },
					"15" : { "name": "entity.name.type.class" },
					"19" : { "name": "entity.name.type.class" }
				}
			}]
		},
		"Comment": {
			"patterns": [{
				"name": "comment.class",
				"match": "#(.*)"
			}]
		},
		"IntValue": {
			"patterns": [{
				"name": "constant.numeric.class",
				"match": "#IntValueRegexString#"
			}]
		},
		"StringValue": {
			"patterns": [{
				"name": "string.quoted.class",
				"match": "\"(((\\\\\\\\)|(\\\\\")|(\\\\n)|([^\"\\n\\\\]))*)\""
			}]
		}
	},
	"scopeName": "source.class"
}