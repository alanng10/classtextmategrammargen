{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Class",
	"patterns": [
		{
			"include": "#Info"
		},
		{
			"include": "#ClassDefine"
		},
		{
			"include": "#WordClass"
		},
		{
			"include": "#Keyword"
		},
		{
			"include": "#IntValue"
		},
		{
			"include": "#StringValue"
		}
	],
	"repository": {
		"ClassDefine": {
			"begin": "#WordBoundaryLeft#(class)#WordBoundaryRight#",
			"beginCaptures": {
				"1": {
					"name": "keyword.class"
				}
			},
			"end": "(?=\\{)",
			"patterns": [
				{
					"include": "#Info"
				},
				{
					"include": "#Keyword"
				},
				{
					"include": "#ClassName"
				}
			]
		},
		"WordClass" : {
			"begin": "#WordBoundaryLeft#(new|share|cast|var|prudate|probate|precate|private)#WordBoundaryRight#",
			"beginCaptures": {
				"1": {
					"name": "keyword.class"
				}
			},
			"end": "(#KeywordRegexString#)|(#NameRegexString#)",
			"endCaptures": {
				"1": {
					"name": "keyword.class"
				},
				"3": {
					"name": "entity.name.type.class"
				}
			},
			"patterns": [
				{
					"include": "#Info"
				}
			]
		},
		"ClassName": {
			"patterns": [{
				"name": "entity.name.type.class",
				"match": "#NameRegexString#"
			}]
		},
		"Keyword": {
			"patterns": [{
				"name": "keyword.class",
				"match": "#KeywordRegexString#"
			}]
		},
		"Info": {
			"patterns": [{
				"name": "comment.class",
				"match": "#(.*)"
			}]
		},
		"IntValue": {
			"patterns": [{
				"name": "constant.numeric.class",
				"match": "#IntValueRegexString#"
			}]
		},
		"StringValue": {
			"patterns": [{
				"name": "string.quoted.class",
				"match": "\"((\\\\\")|([^\"\\n]))*([\"]?)"
			}]
		}
	},
	"scopeName": "source.class"
}